<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
    <servlet>
    <servlet-name>JiveServlet</servlet-name>
    <servlet-class>com.jivesoftware.forum.util.JiveServlet</servlet-class>
    <load-on-startup>0</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>multipartformserv</servlet-name>
    <servlet-class>com.jdon.picture.MultipartFormServ</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>multipartformserv</servlet-name>
    <url-pattern>/multipartformserv</url-pattern>
  </servlet-mapping>
</web-app>
