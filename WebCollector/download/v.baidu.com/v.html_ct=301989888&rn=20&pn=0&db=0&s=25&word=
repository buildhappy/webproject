<script type="text/javascript">
var bdvSugConfig = {
	form: 'bdvSearch',
	input: 'kw',
	num: 6,
	delay: 200
};
	if ( "onhashchange" in window && !navigator.userAgent.match(/MSIE (5|6|7)/) ) {
		bdvSugConfig.openQuickSearch = true;
	}
(function() {
	function loadImg(src) {
		var t = new Date().getTime(),
			img = window['V_fix_img'+t] = new Image();
		img.onload = img.onerror = img.onabort = function() {
			img.onload = img.onerror = img.onabort = null;
			try {
				delete window['V_fix_img'+t];
				img = null;
			} catch(e) {
				img = null;
			}
		}
		img.src = src+'&r='+t;
	};
	var types = {
		movie: 21,
		tv: 22,
		show: 26,
		comic: 43,
		person: 50
	};
	var match = location.search.match(/[&?]q=([^&]+)/);
	if(match){
		document.getElementById('kw').value = decodeURIComponent(match[1]);
	}
	bdvSugConfig.onsubmit = function(evt) {
		loadImg('http://nsclick.baidu.com/v.gif?pid=104&tn=sug&searchpage=indsa&s=zdjs&input=' + encodeURIComponent(evt.query) + '&wd=' + encodeURIComponent(evt.title) + ( evt.index ? '&li=' + evt.index : '' ) + ( evt.eventType ? '&eventtype=' + evt.eventType : '' ));
	};
	bdvSugConfig.onclicklink = function(evt) {
		loadImg('http://nsclick.baidu.com/v.gif?pid=104&tn=sug&s=zdjs&bl=spa&input=' + encodeURIComponent(evt.query) + '&wd=' + encodeURIComponent(evt.title) + '&id=' + evt.id + '&ty=' + types[evt.type] + '&stp=' + evt.target + '&li=' + evt.index + '&u=' + encodeURIComponent(evt.url));
	};
	F.use(['/browse_static/common/lib/tangram/base/base.js','/browse_static/common/ui/vs/search_placeholder/search_placeholder.js'], function(T,searchHolder) {
		var sHolder = new searchHolder('kw');
					//表单序列化
			//@param {string||dom} $form 表单容器
			function serialize($form) {
				var elements = T.dom.query('input', $form),
					params = [];
				if ( elements.length > 0 ) {
					for ( var i = 0, len = elements.length; i < len; i++ ) {
						var item = elements[i];
						if ( item.nodeName.toLowerCase() === 'input' && item.type !== 'submit' && item.name ) {
							params.push(item.name + '=' + encodeURIComponent(item.value));
						}
					}
				}
				return params.join('&');
			}
			if ( "onhashchange" in window && !navigator.userAgent.match(/MSIE (5|6|7)/) ) {
				var isTipInit = false, $input, $tipQS;
				T.on('bdvSearch', 'submit', function(e) {
					T.event.preventDefault(e);
					T.event.stopPropagation(e);
					location.hash = '#' + serialize(T.dom.g('bdvSearch')) + '&ie=utf-8';
				});
				if ( typeof bdvQuickSearch === 'object' ) {
					bdvQuickSearch.on('good', function() {
						if ( !isTipInit ) {
							$input = T.dom.g('kw');
							$tipQS = T.dom.g('tipQS');
							T.on($tipQS, 'click', function() {
								this.style.display = 'none';
								$input.focus();
							});
							T.on($input, 'focus', function() {
								$tipQS.style.display = 'none';
							});
							isTipInit = true;
						}
						$tipQS.style.display = 'block';
					});
				}
				window.onload = function() {
					var $inputBox = T.dom.g('kw'),
						prequery = $inputBox.getAttribute('data-prekey');
					if ( typeof bdvQuickSearch === 'object' && prequery && prequery === $inputBox.value ) {
						bdvQuickSearch.addSubController(prequery);
					}
				};
			} else {
				T.on('bdvSearchBtn', 'click', function(e) {
					var logUrl = 'http://nsclick.baidu.com/v.gif?pid=104&searchpage=indsa&s=zdjs&eventtype=btn_click&wd=' + encodeURIComponent(T.dom.g('kw').value);
					T.event.preventDefault(e);
					T.event.stopPropagation(e);
					if(sHolder.isPrekeyword()){
						logUrl += '&bl=default'
					}
					loadImg(logUrl);
					setTimeout(function() {
						T.dom.g('bdvSearch').submit();
					}, 200);
				});
			}
				if ( document.documentElement.className !== 'quickSearchMod') {
			T.dom.ready(function(){
				sHolder.init('indsa');
			});
		}
	});
}());
document.getElementById('bdvsug_js').src = 'http://list.video.baidu.com/pc_static/open/suggestion/sug-quick-search-min.js?v=' + Math.ceil(new Date() / 7200000);
</script>


        
