<script>
!function(){
   var getHash= function() {
        var match = location.href.match(/#(.*)$/);
        return match ? match[1] : '';
    },
    trim = function(str){
        return str && str.replace(/^\s+|\s+$/g, '');
    }

    var parseUrlParams=  function(uriStr) {
        var _GET = {};
        var paraArr, paraSplit, uriArr, paraStr;
        if (!(uriStr || trim(uriStr))) return _GET;
        if (uriStr.indexOf('?') > -1) {
            uriArr = uriStr.split('?');
            paraStr = uriArr[1];
        } else {
            paraStr = uriStr;
        }
        if (paraStr.indexOf('&') > -1) {
            paraArr = paraStr.split('&');
        } else {
            paraArr = new Array(paraStr);
        }
        for (var i = 0; i < paraArr.length; i++) {
                paraArr[i] = paraArr[i].indexOf('=') > -1 ? paraArr[i] : paraArr[i] + '=';
                paraSplit = paraArr[i].split('=');
            try {
                _GET[paraSplit[0]] = decodeURIComponent(paraSplit[1].replace(/\+/g, ' '));
            } catch (e) {
                _GET[paraSplit[0]] = paraSplit[1].replace(/\+/g, ' ');
            }
        }
        return _GET;
    }
    var word = parseUrlParams(getHash()).word;
    if(word){
        var supportHashChange = "onhashchange" in window;
        if (!supportHashChange || navigator.userAgent.match(/MSIE (5|6|7)/)) 
            location.href = 'http://v.baidu.com/v?fr=quicksearch&ie=utf-8&word='+ word;
        else document.documentElement.className ='quickSearchMod';
    }
}();
</script>

